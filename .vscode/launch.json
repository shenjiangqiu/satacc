    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) booksim main",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/booksim_main",
            "args": ["booksim2/examples/fattree_config"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) test all",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/main.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) test acc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/test/main.out",
            "args": ["acc"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}